{
    "name": "exm",
    "version": "2.0.0",
    "description": "A modern FPS Booster",
    "main": "./out/main/main.js",
    "license": "MIT",
    "scripts": {
        "dev": "electron-vite dev -w --mode development",
        "dev:fast": "electron-vite dev -w --mode development --no-verify",
        "format": "prettier --write .",
        "lint": "eslint . --ext .ts,.tsx --fix",
        "start": "electron-vite preview",
        "check": "npm run lint && npm run format -- --check",
        "postinstall": "electron-builder install-app-deps",
        "vite:build:app": "electron-vite build",
        "electron:build:win": "electron-builder --win",
        "electron:build:dir": "electron-builder --dir",
        "build:unpack": "run-s vite:build:app electron:build:dir",
        "build:win": "run-s vite:build:app electron:build:win",
        "prepare": "husky install"
    },
    "dependencies": {
        "@electron-toolkit/preload": "^3.0.2",
        "@electron-toolkit/utils": "^4.0.0",
        "@hookform/resolvers": "^5.1.1",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.2.5",
        "@radix-ui/react-toggle": "^1.1.9",
        "@radix-ui/react-toggle-group": "^1.1.10",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-react": "^0.511.0",
        "next-themes": "^0.4.6",
        "node-machine-id": "^1.1.12",
        "react-hook-form": "^7.58.1",
        "react-icons": "^5.5.0",
        "react-router-dom": "^7.7.0",
        "sonner": "^2.0.5",
        "tailwind-merge": "^3.3.0",
        "tw-animate-css": "^1.3.2",
        "zod": "^3.25.67",
        "zustand": "^5.0.6"
    },
    "devDependencies": {
        "@electron-toolkit/eslint-config-prettier": "^3.0.0",
        "@electron-toolkit/tsconfig": "^1.0.1",
        "@eslint/js": "^9.28.0",
        "@rushstack/eslint-patch": "^1.11.0",
        "@tailwindcss/vite": "^4.1.8",
        "@types/node": "^22.15.29",
        "@types/react": "^19.1.6",
        "@types/react-dom": "^19.1.5",
        "@vitejs/plugin-react": "^4.5.0",
        "electron": "^36.3.2",
        "electron-builder": "^26.0.12",
        "electron-vite": "^3.1.0",
        "eslint": "^9.28.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "framer-motion": "^12.15.0",
        "husky": "^8.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.5.3",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "tailwindcss": "^4.1.8",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.33.0",
        "vite": "^6.3.5"
    }
}
